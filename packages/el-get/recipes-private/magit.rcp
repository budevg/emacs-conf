(:name magit
       :website "https://github.com/magit/magit#readme"
       :description "It's Magit! An Emacs mode for Git."
       :type github
       :pkgname "magit/magit"
       :branch "master"
       :depends (dash transient with-editor)
       :build `(,(concat "cp lisp/*.el "
                         (in-emacs-d "source_control/magit")))
       )
